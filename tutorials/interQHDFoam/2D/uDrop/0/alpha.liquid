FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alpha.liquid.org;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    leftRightWalls
    {
        type            zeroGradient;
    }
    bottomWalls
    {
        type            zeroGradient;
    }
    topWalls
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }
    #include "wedge.include";
}

