FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];


internalField   uniform (0 0 0);

boundaryField
{
    bottomWalls
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    topWalls
    {
        type            zeroGradient;
    }
    leftRightWalls
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    wedge1
    {
        type            wedge;
    }
    wedge2
    {
        type            wedge;
    }
    defaultFaces
    {
        type            empty;
    }
}


