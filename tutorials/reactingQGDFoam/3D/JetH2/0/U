FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (1305 0 0);
    }

    outlet
    {
        type             zeroGradient;
    }

    "(topAndBottom|back)"
    {
        type            zeroGradient;
    }

    sym
    {
         type             symmetry;
    }

    leftwall
    {
        type            noSlip;
    }
}

